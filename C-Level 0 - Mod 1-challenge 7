import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int v = s.nextInt();
        if (v>0){
System.out.print("+"+v);
}else{
            System.out.print(v);
        }
    }
}
